<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SelecionarObjeto">

    <ImageButton
        android:id="@+id/btnSelecionar"
        android:layout_width="140dp"
        android:layout_height="41dp"
        android:layout_marginStart="132dp"
        android:layout_marginTop="380dp"
        android:background="@null"
        android:contentDescription="@string/btnT4O2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/botao_selecionar" />

    <ImageButton
        android:id="@+id/btnVoltar"
        android:layout_width="140dp"
        android:layout_height="41dp"
        android:layout_marginStart="132dp"
        android:layout_marginTop="20dp"
        android:background="@null"
        android:contentDescription="@string/btnT4O3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnSelecionar"
        app:srcCompat="@drawable/botao_voltar" />
</android.support.constraint.ConstraintLayout>